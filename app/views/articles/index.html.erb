<div class="p-2">
<h1>Articles list</h1> <br>
<ul class="list-group mb-3">
    <% articles.each do |article| %>
        <li class="list-group-item">
        <h4> <%= article.title %> - <%= article.created_at %> </h4> <br>
        <p> <%= article.body %> </p> <br>
        <span>id: <%= article.id %> </span>
        </li>
    <% end %>
</ul>
</div>
